{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "rewriteRelativeImportExtensions": true,
    "types": ["@cloudflare/workers-types", "bun"],
    "skipLibCheck": true,
    "noEmit": true
  }
}
